  {
    "end_time": 166164,
    "index": 64,
    "start_time": 161164,
    "text": "(&quot;Allure Amour&quot;)"
  },
  {
    "end_time": 168080,
    "index": 65,
    "start_time": 166293,
    "text": "- I lost five pounds."
  },
  {
    "end_time": 168852,
    "index": 66,
    "start_time": 168080,
    "text": "That&#39;s crazy."
  },
  {
    "end_time": 170009,
