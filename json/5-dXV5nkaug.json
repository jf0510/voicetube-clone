  },
  {
    "end_time": 80830,
    "index": 34,
    "start_time": 79496,
    "text": "OH, NO!"
  },
  {
    "end_time": 85042,
    "index": 35,
    "start_time": 82582,
    "text": "UH, H-HEY, MAN.\nHEY, HEY, SORRY.\nQUICK QUESTION."
  },
  {
    "end_time": 87336,
    "index": 36,
    "start_time": 85085,
    "text": "UH, WHAT SHOULD I DO\nIF THE WATER IN THE TOILET"
  },
  {
    "end_time": 89547,
    "index": 37,
    "start_time": 87378,
    "text": "ISN&#39;T IN THE TOILET\nANYMORE?"
  },
  {
    "end_time": 91257,
    "index": 38,
    "start_time": 89589,
    "text": "ACTUALLY, WAIT.\nI-I-I THINK I GOT IT."
  },
  {
    "end_time": 92592,
    "index": 39,
    "start_time": 91299,
    "text": "SORRY. GOOD NIGHT."
  },
  {
    "end_time": 94010,
    "index": 40,
    "start_time": 92634,
    "text": "[ WATER TRICKLING ]"
  },
  {
    "end_time": 95303,
    "index": 41,
    "start_time": 94052,
    "text": "OH, MAN."
  },
  {
    "end_time": 98223,
    "index": 42,
    "start_time": 95345,
    "text": "[ PLUNGER THUDDING,\nMETAL CLINKS ]"
  },
  {
    "end_time": 100433,
    "index": 43,
    "start_time": 98264,
    "text": "UPDATE -- SO, NOW THE WATER\nIS SORT OF EVERYWHERE,"
  },
  {
    "end_time": 102184,
    "index": 44,
