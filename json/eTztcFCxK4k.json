  },
  {
    "end_time": 92220,
    "index": 24,
    "start_time": 89480,
    "text": "in the way I can&#39;t understand"
  },
  {
    "end_time": 96240,
    "index": 25,
    "start_time": 92320,
    "text": "I want to live as a child in the Neverland"
  },
  {
    "end_time": 100380,
    "index": 26,
    "start_time": 96380,
    "text": "Where baddies will get punished for their horrible lies"
  },
  {
    "end_time": 104380,
    "index": 27,
    "start_time": 100520,
