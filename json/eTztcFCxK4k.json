  },
  {
    "end_time": 71700,
    "index": 17,
    "start_time": 70560,
    "text": "corroding my mind"
  },
  {
    "end_time": 73760,
    "index": 18,
    "start_time": 71760,
    "text": "Can&#39;t even feel the hate anymore"
  },
  {
    "end_time": 75700,
    "index": 19,
