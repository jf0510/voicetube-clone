[
    {
        "index": 1,
        "start_time": 587,
        "end_time": 3794,
        "text": "Trembling stars are shining"
    },
    {
        "index": 2,
        "start_time": 3794,
        "end_time": 7475,
        "text": "Where are you looking?"
    },
    {
        "index": 3,
        "start_time": 7475,
        "end_time": 12144,
        "text": "They look like they will \ndisappear at any moment"
    },
    {
        "index": 4,
        "start_time": 27509,
        "end_time": 33045,
        "text": "Turn on the light \nMy heart is dark"
    },
    {
        "index": 5,
        "start_time": 33045,
        "end_time": 37773,
        "text": "I feel morose today, too"
    },
    {
        "index": 6,
        "start_time": 37773,
        "end_time": 40571,
        "text": "In your mind"
    },
    {
        "index": 7,
        "start_time": 40571,
        "end_time": 42827,
        "text": "I\u2019m not who I feel"
    },
    {
        "index": 8,
        "start_time": 42827,
        "end_time": 49107,
        "text": "When did I lose you?"
    },
    {
        "index": 9,
        "start_time": 49107,
        "end_time": 55156,
        "text": "As I like you the stars rise"
    },
    {
        "index": 10,
        "start_time": 55156,
        "end_time": 60452,
        "text": "I still don\u2019t know what to do"
    },
    {
        "index": 11,
        "start_time": 60452,
        "end_time": 65725,
        "text": "Mysterious you in your \ntone veiled in a mist"
    },
    {
        "index": 12,
        "start_time": 65725,
        "end_time": 71284,
        "text": "The night you come to my mind"
    },
    {
        "index": 13,
        "start_time": 71284,
        "end_time": 74356,
        "text": "Trembling stars are shining"
    },
    {
        "index": 14,
        "start_time": 74356,
        "end_time": 78076,
        "text": "Where are you looking?"
    },
    {
        "index": 15,
        "start_time": 78076,
        "end_time": 82324,
        "text": "They look like they will \ndisappear at any moment"
    },
    {
        "index": 16,
        "start_time": 82324,
        "end_time": 85483,
        "text": "After the time that I\u2019ve waited"
    },
    {
        "index": 17,
        "start_time": 85483,
        "end_time": 87115,
        "text": "I\u2019ll let you go now"
    },
    {
        "index": 18,
        "start_time": 87115,
        "end_time": 90987,
        "text": "No the night I still want to"
    },
    {
        "index": 19,
        "start_time": 90987,
        "end_time": 94315,
        "text": "Secretly keep you in my room"
    },
    {
        "index": 20,
        "start_time": 94315,
        "end_time": 99644,
        "text": "At night night night\nI fly to the sky"
    },
    {
        "index": 21,
        "start_time": 99644,
        "end_time": 102427,
        "text": "As time time for the moon night"
    },
    {
        "index": 22,
        "start_time": 102427,
        "end_time": 105712,
        "text": "Shall I meet you in my dream"
    },
    {
        "index": 23,
        "start_time": 108670,
        "end_time": 114399,
        "text": "And confess my \nardent love? (Shall I?)"
    },
    {
        "index": 24,
        "start_time": 114399,
        "end_time": 119751,
        "text": "I keep calling you\nthrough the window"
    },
    {
        "index": 25,
        "start_time": 119751,
        "end_time": 124999,
        "text": "This way I want us to be closer"
    },
    {
        "index": 26,
        "start_time": 124999,
        "end_time": 130558,
        "text": "The night I want to expect it"
    },
    {
        "index": 27,
        "start_time": 130558,
        "end_time": 133662,
        "text": "Trembling stars are shining"
    },
    {
        "index": 28,
        "start_time": 133662,
        "end_time": 137335,
        "text": "Where are you looking?"
    },
    {
        "index": 29,
        "start_time": 137335,
