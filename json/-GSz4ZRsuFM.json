[
  {
    "end_time": 2843,
    "index": 1,
    "start_time": 0,
    "text": "- Diarrhea is coming for you."
  },
  {
    "end_time": 4418,
    "index": 2,
    "start_time": 2843,
    "text": "- It&#39;s coming for you too, Claire."
  },
  {
    "end_time": 6918,
    "index": 3,
    "start_time": 4418,
    "text": "(funky music)"
  },
  {
    "end_time": 16570,
    "index": 4,
    "start_time": 15504,
    "text": "- I love spicy food."
  },
  {
    "end_time": 18229,
    "index": 5,
    "start_time": 16570,
    "text": "I&#39;ve never had spicy ramen before,"
  },
  {
    "end_time": 21217,
    "index": 6,
