[
  {
    "end_time": 5337,
    "index": 1,
    "start_time": 4086,
    "text": "[ RUSTLING ]\nAAH! DID YOU HEAR THAT?"
  },
  {
    "end_time": 6881,
    "index": 2,
    "start_time": 5380,
    "text": "HEAR WHAT?"
  },
  {
    "end_time": 8550,
    "index": 3,
    "start_time": 6923,
    "text": "[ RUSTLING CONTINUES ]\nAAH! THERE IT IS AGAIN!"
  },
  {
    "end_time": 11677,
    "index": 4,
    "start_time": 10510,
    "text": "HELLO?"
  },
  {
    "end_time": 13263,
    "index": 5,
    "start_time": 11720,
    "text": "[ RUSTLING ]\n[ ALL SCREAM ]"
  },
  {
    "end_time": 14764,
    "index": 6,
    "start_time": 13304,
    "text": "I THINK\nTHE SLUGMAN&#39;S AFTER US."
  },
  {
    "end_time": 15764,
    "index": 7,
    "start_time": 14806,
    "text": "I DON&#39;T WANT TO BE\nSLUG FOOD!"
  },
  {
    "end_time": 16850,
    "index": 8,
    "start_time": 15807,
    "text": "SHH!\nDON&#39;T LET HIM HEAR US!"
  },
  {
    "end_time": 18226,
    "index": 9,
    "start_time": 16891,
    "text": "ICE BEAR WANTS TO LIVE."
  },
  {
    "end_time": 20186,
    "index": 10,
    "start_time": 18268,
    "text": "PANDA, LISTEN!\nTHERE IS NO SLUG MAN --"
  },
  {
    "end_time": 22939,
    "index": 11,
    "start_time": 20228,
    "text": "THAT WAS JUST AN OLD STORY\nWE USED TO TELL AT SUMMER CAMP."
  },
  {
    "end_time": 24149,
    "index": 12,
    "start_time": 22981,
    "text": "BUT THE NOISES."
  },
  {
    "end_time": 25150,
    "index": 13,
    "start_time": 24190,
    "text": "IT&#39;S JUST THE RAIN."
  },
  {
    "end_time": 26692,
    "index": 14,
    "start_time": 25191,
    "text": "[ THUD, GLASS SHATTERS ]"
  },
  {
    "end_time": 30572,
    "index": 15,
    "start_time": 29320,
    "text": "[ ALL SCREAM ]\nIT&#39;S THE SLUG MAN!"
  },
  {
    "end_time": 31740,
    "index": 16,
    "start_time": 30613,
    "text": "GRAB CHLOE!\nMOVE, MOVE!"
  },
  {
    "end_time": 32741,
    "index": 17,
    "start_time": 31781,
    "text": "WAIT! WAIT UP!"
  },
  {
    "end_time": 34284,
    "index": 18,
    "start_time": 32782,
    "text": "WHOA, GUYS! WAIT!"
  },
  {
    "end_time": 38538,
    "index": 19,
    "start_time": 36494,
    "text": "OKAY, SECURE THE ROOM!\nSTAY RIGHT HERE, CHLOE!"
  },
  {
    "end_time": 39748,
    "index": 20,
    "start_time": 38580,
    "text": "CONTAINING THE CHLOE."
  },
  {
    "end_time": 42208,
    "index": 21,
    "start_time": 39789,
    "text": "WAIT, BUT WE --\nHEY, GUYS -- UGH!"
  },
  {
    "end_time": 43793,
    "index": 22,
    "start_time": 42250,
    "text": "DON&#39;T WORRY, CHLOE.\nWE GOT YOU."
  },
  {
    "end_time": 45295,
    "index": 23,
    "start_time": 43835,
    "text": "STOP!\nJUST LISTEN!"
  },
  {
    "end_time": 47088,
    "index": 24,
    "start_time": 45336,
    "text": "NO. IT&#39;S NOT SAFE\nOUT THERE, CHLOE."
  },
  {
    "end_time": 49507,
    "index": 25,
    "start_time": 47130,
    "text": "YOU ARE OUR SWEET BABY ANGEL,\nAND WE MADE A PROMISE"
  },
  {
    "end_time": 51801,
    "index": 26,
    "start_time": 49549,
    "text": "TO YOUR FATHER TO KEEP YOU SAFE.\nIT&#39;S OUR DUTY."
  },
  {
    "end_time": 54095,
    "index": 27,
    "start_time": 51843,
    "text": "GRIZZ, COME ON.\nIT&#39;S THE RAIN MAKING THAT NOISE."
  },
  {
    "end_time": 56263,
    "index": 28,
    "start_time": 54345,
    "text": "HUSH, SWEET CHILD.\nYOU&#39;RE DELIRIOUS FROM FEAR."
  },
  {
    "end_time": 57766,
    "index": 29,
    "start_time": 56306,
    "text": "WE&#39;VE GOT THIS\nCOMPLETELY UNDER CONTROL."
  },
  {
    "end_time": 59184,
    "index": 30,
    "start_time": 57807,
    "text": "I&#39;M JUST GONNA\nBORROW THIS FOR A SEC."
  },
  {
    "end_time": 60685,
    "index": 31,
    "start_time": 59225,
    "text": "I&#39;M GONNA GIVE YOUR DAD\nA QUICK UPDATE."
  },
  {
    "end_time": 61769,
    "index": 32,
    "start_time": 60727,
    "text": "HUH?"
  },
  {
    "end_time": 62979,
    "index": 33,
    "start_time": 61811,
    "text": "[ GASPS ] NO SERVICE?"
  },
  {
    "end_time": 64314,
    "index": 34,
    "start_time": 63021,
    "text": "PANPAN,\nCALL MR. CHLOE&#39;S DAD."
  },
  {
    "end_time": 66316,
    "index": 35,
    "start_time": 64355,
    "text": "YEAH, SURE."
  },
  {
    "end_time": 69860,
    "index": 36,
    "start_time": 67609,
    "text": "I THINK MY PHONE IS STILL\nIN THE LIVING ROOM, GRIZZ."
  },
  {
    "end_time": 71780,
    "index": 37,
    "start_time": 69903,
    "text": "I CAN GO GET IT.\nGOOD IDEA, CHLOE."
  },
  {
    "end_time": 73239,
    "index": 38,
    "start_time": 71821,
    "text": "ONE OF US --\nNOT YOU, THOUGH --"
  },
  {
    "end_time": 74741,
    "index": 39,
    "start_time": 73281,
    "text": "IS GOING TO HAVE TO\nINVESTIGATE."
  },
  {
    "end_time": 77285,
    "index": 40,
    "start_time": 74783,
    "text": "THE PRETZELS\nWILL DECIDE OUR FATE."
  },
  {
    "end_time": 78369,
    "index": 41,
    "start_time": 77327,
    "text": "LITTLE BRO."
  },
  {
    "end_time": 79829,
    "index": 42,
    "start_time": 78411,
    "text": "PANPAN."
  },
  {
    "end_time": 81372,
    "index": 43,
    "start_time": 79871,
    "text": "[ WHIMPERS ]"
  },
  {
    "end_time": 82749,
    "index": 44,
    "start_time": 81414,
    "text": "BE STRONG, PANDA."
  },
  {
    "end_time": 85210,
    "index": 45,
    "start_time": 82791,
    "text": "ICE BEAR WILL TELL TALE\nOF BRAVE BROTHER."
  },
  {
    "end_time": 86753,
    "index": 46,
    "start_time": 85251,
    "text": "OH, PLEASE."
  }
]