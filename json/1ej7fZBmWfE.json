    {
        "index": 32,
        "start_time": 187180,
        "end_time": 190654,
        "text": "I\u2019m afraid of packing luggages."
    },
    {
        "index": 33,
        "start_time": 190654,
        "end_time": 194800,
        "text": "I\u2019m afraid of sleeping without lights"
    },
    {
        "index": 34,
        "start_time": 194800,
        "end_time": 202240,
        "text": "I\u2019m afraid of blinking; I\u2019d miss you if you were here"
    },
    {
        "index": 35,
        "start_time": 202780,
        "end_time": 206080,
        "text": "I\u2019m afraid of the sea of people."
    },
    {
        "index": 36,
        "start_time": 206080,
        "end_time": 210140,
        "text": "I\u2019m afraid of the streamlets winding down the heights."
    },
    {
        "index": 37,
        "start_time": 210140,
        "end_time": 217100,
        "text": "I\u2019m afraid that I can\u2019t find you even though I\u2019m near"
    },
    {
        "index": 38,
        "start_time": 217320,
        "end_time": 224720,
        "text": "If I fell into the bottom of the ocean suddenly,"
    },
    {
        "index": 39,
        "start_time": 224860,
        "end_time": 231500,
        "text": "Would you sense where I was sinking?"
    },
    {
        "index": 40,
        "start_time": 232420,
        "end_time": 239780,
        "text": "Don\u2019t mind me. I was just saying."
    },
    {
        "index": 41,
        "start_time": 240100,
        "end_time": 247080,
        "text": "Don\u2019t feel pressured. I only want to see you briefly."
    },
    {
        "index": 42,
        "start_time": 255780,
        "end_time": 259223,
        "text": "I\u2019m afraid of your breathing."
    },
    {
        "index": 43,
        "start_time": 259223,
        "end_time": 263260,
        "text": "I\u2019m afraid of the narrow distance."
    },
    {
        "index": 44,
        "start_time": 263280,
        "end_time": 273160,
        "text": "I\u2019m afraid that someone else will mention your secrets."
    },
    {
        "index": 45,
        "start_time": 285680,
        "end_time": 291380,
        "text": "&quot;Love&quot; acts like nothing matters."
